		if(a == 2) {
			puts(str2);
		}
	}
	a = 3;
	b:Int = 3;
	while(a > 0) {
		while(b > 0) {
			puts(str1);
			b = b - 1;
			if(b == 2) {
				puts(str2);
			}
		}
		a = a - 1;
		
	}
	puts("fiM");
	return 2;
}

