decl printInt(i:Int):Void
decl printFloat(f:Float):Void
decl printString(s:String):Void


def main(ar:[String]):Int { 
	a:Int = 3;
	printInt(3+a-a);
	printInt(3+a);
	printInt(a*2);
	printInt(a*a);
	printInt(a/a);	
	printInt(-3);
	printInt(a%3);
	
	printString("----");
	
	b:Float = 3.0;
	printFloat(3.0+b-b);
	printFloat(3.0+b);
	printFloat(b*2.0);
	printFloat(b*b);
	printFloat(b/b);
	printFloat(-3.0);	

	
	return 0;
}