#whitespace insensitive demonstration

def 

                  fib
                  
                  
                  (i       :           Int):
                  Int 
                  {
	if 
	i
	 <=
	 
	  2 
	  {
		return 
		1;
	}
	return 
	
	fib(i-1) 
	+
	 fib(i
	 
	 -
	 2);
}


def 

factorial           (i  :       Int)           :        Int         {
	r            :             Int        =        1;
	a:Int=1;
	while        a      <=       i        {
		r     =      r     *      i    ;
		        a       =       a      +       1     ;
	
	
}
	return  
	r
	;
}